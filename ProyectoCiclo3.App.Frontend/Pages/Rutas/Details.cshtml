@page
@model ProyectoCiclo3.App.Frontend.Pages.DetailsRutaModel
@{
    var ruta = Model.Ruta;
}
 
<ul class="list-group list-group-flush">
  <li class="list-group-item"> <b>id: </b> @ruta.id</li>
  <li class="list-group-item"> <b>Origen: </b> @ruta.nombre</li>
  <li class="list-group-item"> <b>Destino: </b> @ruta.ciudad</li>
  <li class="list-group-item"><b>Tiempo estimado: </b> @ruta.pais</li>
</ul>
